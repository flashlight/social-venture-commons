<h1>Editing skill_tag</h1>

<% form_for(@skill_tag) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :user %><br />
    <%= f.text_field :user %>
  </p>
  <p>
    <%= f.label :need %><br />
    <%= f.text_field :need %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @skill_tag %> |
<%= link_to 'Back', skill_tags_path %>
